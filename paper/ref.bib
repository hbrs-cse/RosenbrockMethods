@article{rang12,
title = {A stiffly accurate Rosenbrock-type method of order 2 applied to FE-analyses in finite strain viscoelasticity},
journal = {Applied Numerical Mathematics},
volume = {62},
number = {12},
pages = {1837-1848},
year = {2012},
issn = {0168-9274},
doi = {10.1016/j.apnum.2012.08.001},
url = {https://www.sciencedirect.com/science/article/pii/S016892741200133X},
author = {Ahmad-Wahadj Hamkar and Stefan Hartmann and Joachim Rang},
keywords = {Rosenbrock-type methods, Differential-algebraic equations},
abstract = {The numerical treatment of quasi-static problems in solid mechanics incorporating constitutive equations of evolutionary type is pursued using the method of vertical lines. The spatial discretization is carried out by means of the finite element method. This yields large systems of differential-algebraic equations. In this article a new stiffly accurate Rosenbrock-type method of second order is proposed, which is applied to this challenging test case, and comparisons to existing Rosenbrock-type and diagonally-implicit Runge–Kutta methods are carried out.}
}

